<tr>
    <td><strong><%= link_to classification.associationID, edit_administrator_classification_path(classification.associationID), class: "link-dark link-offset-2 link-underline-opacity-25 link-underline-opacity-100-hover" %></strong></td>
    <td><%= classification.category.cat_name %></td>
    <td><%= classification.organization.org_name %></td>
    <td><%= classification.created_at %></td>
    <td><%= classification.updated_at %></td>
    <td><%= button_to "Delete", classification, method: :delete, class: 'btn btn-outline-success', onclick: "return confirm('Are you sure you want to delete this Classification?');" %></td>
</tr>